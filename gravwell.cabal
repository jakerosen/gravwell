cabal-version: 2.4

name: gravwell
version: 0

executable gravwell
  build-depends:
      base
    , containers
  default-extensions:
    BlockArguments
    DerivingStrategies
    LambdaCase
    TypeApplications
    ScopedTypeVariables
  default-language: Haskell2010
  ghc-options: -Wall
  main-is: Main.hs
  other-modules:
    Game
    Card
